    "pocketbase": "^0.19.0",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.2",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.2",
